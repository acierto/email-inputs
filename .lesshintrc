{
    "fileExtensions": [".less", ".css"],
    "idSelector": false,
    "hexLength": "short",
    "maxCharPerLine": 120,
    "onlyVariableColors": true,
    "qualifyingElement": {
        "allowWithAttribute": true,
        "allowWithClass": true,
        "allowWithId": true
    },
    "spaceAroundComma": false,
    "universalSelector": false,
    "zeroUnit": "no_unit"
}
