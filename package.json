{
  "author": "Bogdan Nechyporenko",
  "description": "Lightweight email inputs component without dependencies",
  "name": "email-inputs",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:acierto/email-inputs.git"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "ascii-table": "0.0.9",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-root-import": "6.5.0",
    "bean": "1.0.15",
    "console-error-throws-error": "1.2.1",
    "css-loader": "3.5.3",
    "del": "5.1.0",
    "doiuse": "4.2.0",
    "eslint": "7.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "expose-loader": "0.7.5",
    "gh-pages": "2.2.0",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-env": "0.4.0",
    "gulp-eslint": "6.0.0",
    "gulp-jest": "4.0.3",
    "gulp-lesshint": "6.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-protractor": "4.1.1",
    "gulp-release-it": "2.1.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "jasmine-collection-matchers": "0.2.0",
    "jasmine-fail-fast": "2.0.1",
    "jasmine-object-matchers-jest": "0.1.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "25.0.0",
    "jest-cli": "25.0.0",
    "less-loader": "6.1.0",
    "postcss-loader": "3.0.0",
    "protractor-base-dsl": "0.1.35",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "protractor-screenshot-reporter": "0.0.5",
    "selenium-standalone": "6.17.0",
    "strip-sourcemap-loader": "0.0.1",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-dev-server": "3.11.0",
    "webpack-stream": "5.2.1",
    "yargs": "15.3.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/component/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/build/reports/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/file-mock.js",
      "\\.(css|less)$": "<rootDir>/tests/mocks/style-mock.js"
    },
    "notify": true,
    "roots": [
      "<rootDir>/tests/unit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/globals/jasmine.conf.js"
    ],
    "testRegex": "/.*-spec\\.(js)$",
    "verbose": true
  }
}
